{
  "name": "houser-js-toolbox",
  "version": "0.0.1",
  "description": "My javascript toolbox, you can say it is a collection of wheel.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "test",
    "start": "nodemon --ext ts --watch lib --exec \"npm run clean && npm run build:ts && npm run server\"",
    "server": "cross-env NODE_ENV=development node ./build/demo/index.js",
    "clean": "rm -rf ./build",
    "build:webpack": "webpack --config webpack.config.js",
    "build:ts": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Houserqu/houser-js-toolbox.git"
  },
  "keywords": [
    "toolbox",
    "js"
  ],
  "author": "houserqu",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "typescript": "^3.2.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "nodemon": "^1.18.9"
  }
}
